        <xsd:attribute type="aa:System.String" name="Property" />
        <xsd:attribute type="aa:System.String" name="Value" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CrossX.Framework.ApplicationDefinition.Style">
    <xsd:complexContent>
      <xsd:extension base="xx:System.Object">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
          <xsd:element name="Setter" type="xx:CrossX.Framework.ApplicationDefinition.Setter" minOccurs="0" maxOccurs="unbounded" />
        </xsd:choice>
        <xsd:attribute type="aa:System.String" name="Selector" />
