  </xsd:element>
  <xsd:element name="Transform">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="xx:CrossX.Framework.ApplicationDefinition.Transform" />
      </xsd:complexContent>
    </xsd:complexType>
